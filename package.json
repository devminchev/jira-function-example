{
  "name": "jira-function-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.29.1",
    "@contentful/f36-components": "^4.78.0",
    "@contentful/f36-multiselect": "^4.23.1",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@tanstack/react-query": "^5.59.0",
    "contentful-management": "11.50.0",
    "emotion": "10.0.27",
    "graphql": "^16.11.0",
    "graphql-request": "^7.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.8"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && npm run build:functions",
    "preview": "vite preview",
    "test": "vitest",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "add-locations": "contentful-app-scripts add-locations",
    "upsert-actions": "contentful-app-scripts upsert-actions --ci --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "build:functions": "contentful-app-scripts build-functions --ci",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@contentful/app-scripts": "^2.3.0",
    "@contentful/node-apps-toolkit": "^3.11.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@tsconfig/create-react-app": "^2.0.5",
    "@tsconfig/recommended": "1.0.8",
    "@types/node": "^20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.0.3",
    "cross-env": "7.0.3",
    "gh-pages": "^6.1.1",
    "jsdom": "^26.0.0",
    "typescript": "^5.4.5",
    "vite": "^6.2.2",
    "vitest": "^3.0.9"
  },
  "homepage": "."
}
